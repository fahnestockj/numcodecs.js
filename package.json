{
  "name": "numcodecs",
  "author": "Trevor James Manz",
  "version": "0.0.0",
  "description": "Buffer compression and transformation codecs for use in data storage and communication applications.",
  "module": "dist/numcodecs.esm.js",
  "browser": "dist/numcodecs.umd.js",
  "repository": {
    "type": "git",
    "url": "github.com/manzt/numcodecs"
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly --declarationDir ./dist/types && rollup -c",
    "prebuild": "rm -rf dist",
    "lint": "tsc --noEmit && eslint ./src/**/*.ts --quiet --fix",
    "format": "./node_modules/.bin/prettier --write src/**/*.ts",
    "format:check": "./node_modules/.bin/prettier --check src/**/*.ts",
    "start": "rollup -c -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run format:check && npm run test -- --no-cache"
  },
  "keywords": [
    "zarr",
    "compression",
    "codecs",
    "storage"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/jest": "^25.1.3",
    "@types/pako": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "1.19.1",
    "rollup": "^1.32.0",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "pako": "^1.0.11"
  }
}
